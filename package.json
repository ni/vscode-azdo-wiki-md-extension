{
	"name": "azdo-wiki-md",
	"version": "0.0.4",
	"displayName": "NI Markdown Tools for Microsoft Azure DevOps Wiki Files",
	"description": "A VS Code extension that makes your local editing of Azure DevOps Wiki files easier.",
	"keywords": [
		"markdown",
		"AzDO",
		"Azure DevOps",
		"wiki"
	],
	"icon": "docs/logo.png",
	"publisher": "ni",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/ni/vscode-azdo-wiki-md-extension"
	},
	"bugs": {
		"url": "https://github.com/ni/vscode-azdo-wiki-md-extension/issues"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"contributes": {
		"markdown.markdownItPlugins": true,
		"configuration": {
			"type": "object",
			"title": "Azure DevOps Wiki Markdown",
			"properties": {
				"azdo-wiki-md.token": {
					"type": [
						"string"
					],
					"description": "Your AzDO Personal Access Token. Must have Read Identity permissions",
					"scope": "window"
				},
				"azdo-wiki-md.organization": {
					"type": [
						"string"
					],
					"description": "Your AzDO Organization name.",
					"scope": "window"
				}
			}
		},
		"commands": [{
			"command": "extension.insertAzDOUserGUID",
			"title": "Insert AzDO User GUID"
		}]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"deploy": "vsce publish"
	},
	"devDependencies": {
		"@types/markdown-it": "^10.0.1",
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^3.9.4",
		"vsce": "^1.77.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"markdown-it-emoji": "^1.4.0"
	}
}
